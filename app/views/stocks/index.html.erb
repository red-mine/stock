<h1>Stocks</h1>
<table>
<tr>
  <th>stock</th>
  <th>coef</th>
  <th>price</th>
  <th>good</th>
</tr>
<% @stocks.each do |stock| %>
  <tr>
    <td>
      <a href="/stocks/<%= stock.stock %>">
        <%= stock.stock %>
      </a>
    </td>
    <td><%= stock.coef.round(2) %></td>
    <td><%= stock.price %></td>
    <td><%= stock.good %></td>
  </tr>
<% end %>
</table>

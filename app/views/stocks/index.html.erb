<h1>Stocks - SZ</h1>

<h2>Date - STAVE - <%= @stavs_date %></h1>

<table border=1 cellspacing=0>

<tr align=left>
  <th>Stock/SZ</th>
  <th>Coef</th>
  <th>Year</th>
  <th>Price</th>
  <th>STAVE</th>
</tr>

<% @stocks_stavs.each do |stock| %>
<tr>
  <td>
    <a href="/stocks/<%= stock.stock %>/<%= stock.years %>">
      <%= stock.stock.upcase %>
    </a>
  </td>
  <td><%= stock.coef.round(2) %></td>
  <td><%= stock.year.round(2) %></td>
  <td><%= stock.price %></td>
  <td><%= stock.stave %></td>
</tr>
<% end %>

</table>

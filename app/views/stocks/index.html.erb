<h1>Stocks - SZ</h1>

<h2>Date - STAVE - <%= @stavs_date %></h1>

<table>
<tr>

<td>
<table border=1 cellspacing=0>

<tr align=left>
  <th>Stock/SZ</th>
  <th>Loh</th>
  <th>One</th>
  <th>Price</th>
  <th>LOHAS</th>
  <th>YEARS</th>
</tr>

<% @stocks_stavs.each do |stock| %>
<tr>

  <td><%= stock.stock.upcase %></td>

  <td>
    <a href="/stocks/<%= stock.stock %>/<%= 857 %>">
      <%= stock.loha.round(2) %>
    </a>
  </td>

  <td>
    <a href="/stocks/<%= stock.stock %>/<%= 250 %>">
      <%= stock.year.round(2) %>
    </a>
  </td>

  <td><%= stock.price %></td>

  <td><%= stock.lohas %></td>
  <td><%= stock.years %></td>

</tr>
<% end %>

</table>
</td>

<td valign=top>
<img src="stave/stave.png" style="max-width: 80%;">
</td>

</tr>
</table>